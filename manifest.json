{
  "manifest_version": 3,
  "name": "Codeforces Submission Analyzer",
  "version": "1.0",
  "description": "Analyze accepted Codeforces submissions with Together AI to get complexity and efficiency suggestions.",
  "permissions": [
    "storage",
    "https://codeforces.com/*",
    "https://api.together.xyz/*"
  ],
  "host_permissions": [
    "https://codeforces.com/*",
    "https://api.together.xyz/*"
  ],
  "action": {
    "default_popup": "popup.html"
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}